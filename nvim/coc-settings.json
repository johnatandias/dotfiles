{
  "tsserver.trace.server": "verbose",
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact"
  ],
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact"
  ],
  "explorer.icon.enableNerdfont": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableVimDevicons": false,
  "explorer.previewAction.onHover": false,
  "explorer.file.column.git.showIgnored": true,
  "explorer.quitOnOpen": true,
  "explorer.icon.customIcons": {
    "icons": {
      "npm": {
        "code": " ",
        "color": "#9c371e"
      },
      "folderClosed": {
        "code": " ",
        "color": "#90A4AE"
      },
      "folderOpened": {
        "code": " ",
        "color": "#90A4AE"
      },
      "css": {
        "code": " ",
        "color": "#42A5F5"
      },
      "javascript": {
        "code": " ",
        "color": "#FFCA28"
      },
      "html": {
        "code": " ",
        "color": "#E44D26"
      },
      "github": {
        "code": " ",
        "color": "#90A4AE"
      },
      "bitbucket": {
        "code": " ",
        "color": "#90A4AE"
      },
      "gitlab": {
        "code": " ",
        "color": "#E24329"
      },
      "env": {
        "code": " ",
        "color": "#90A4AE"
      },
      "markdown": {
        "code": " ",
        "color": "#90A4AE"
      }
    },
    "extensions": {
      "md": "markdown"
    },
    "filenames": {
      "bitbucket-pipelines.yml": "bitbucket",
      ".gitlab-ci.yml": "gitlab",
      ".env": "env",
      ".env.prod": "env",
      ".env.staging": "env",
      ".env.test": "env"
    },
    "dirnames": {
      "node_modules": "npm",
      ".github": "github"
    }
  },
  "explorer.keyMappings": {
    "o": ["expanded?", "collapse", "expand"],
    "v": "open:vsplit",

    "y": "copyFilepath",
    "Y": "copyFilename",
    "c": "copyFile",
    "x": "cutFile",
    "p": "pasteFile",
    "d": "delete",
    "D": "deleteForever",

    "a": "addFile",
    "A": "addDirectory",
    "r": "rename",

    "R": "refresh"
  }
}
